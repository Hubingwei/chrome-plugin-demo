{
  "manifest_version": 2,
  "name": "bookmarks",
  "version": "1.0.0",
  "description": "bookmarks示例",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "./js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "bookmarks示例插件",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ]
}